{
  "name": "toxic-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./jest.config.js",
    "typeorm": "typeorm-ts-node-commonjs -d src/config/ormconfig.ts",
